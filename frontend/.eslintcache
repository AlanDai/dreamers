m_actions.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/actions/search_actions.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/actions/tag_actions.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/actions/modal_actions.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/actions/user_actions.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/session/session_form.jsx",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/nav/navbar.jsx",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/profile/profile.jsx",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/feed/feed.jsx",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/tags/tags.jsx",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/dreams/new_dream_container.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/feed/comment_dream_modal.jsx",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/util/search_api_util.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/util/tag_api_util.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/util/dream_api_util.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/util/comment_api_util.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/util/user_api_util.js",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/feed/dream_item.jsx",[],"/Users/cindyli/Desktop/mern-sample/dreamers/frontend/src/components/dreams/new_dream.jsx",["219","220"],"import React from 'react';\n\nclass NewDream extends React.Component {\n    constructor(props) {\n        super(props);\n        if (Object.values(this.props.info).length === 0) {\n            // debugger;\n            this.state = {\n                selectedOption: 'dream',\n                dreamText: '',\n                searchValue: '',\n                tags: ['KillingIt', 'Love', 'Teeth'],\n                // newTags: this.props.tags\n            }\n        } else {\n            //debugger;\n            this.state = {\n                selectedOption: this.props.info.type,\n                dreamText: this.props.info.text,\n                searchValue: '',\n                tags: this.props.info.tags,\n            }\n        }\n        this.handleChange = this.handleChange.bind(this);\n        this.handleSubmit = this.handleSubmit.bind(this);\n        this.handleTags = this.handleTags.bind(this);\n        this.removeTag = this.removeTag.bind(this);\n    }\n\n    handleChange(type) {\n        return (e) => {\n            this.setState({ [type]: e.currentTarget.value })\n        }\n    }\n\n    handleSubmit(e) {\n        e.preventDefault();\n        let newDream = {\n            username: this.props.currentUser.username,\n            userId: this.props.currentUser.id,\n            type: this.state.selectedOption,\n            text: this.state.dreamText,\n            tags: this.state.tags\n        }\n        // debugger;\n        if (this.props.info.userId) {\n            this.props.updateDream(this.props.info._id, newDream);\n            // console.log('update dream', { dream: newDream })\n        } else {\n            // console.log('new dream', newDream)\n            // this.props.createDream({dream: newDream});\n            this.props.createDream(newDream);\n        }\n\n        this.props.closeModal()\n        \n        //!needs to be fixed \n        window.location.reload();\n        // console.log('new dream', newDream)\n        // this.props.closeModal();\n    }\n\n    handleTags() {\n        let newTags = this.state.tags\n        newTags.push(this.state.searchValue)\n        this.setState({ tags: newTags })\n        \n    }\n\n    removeTag(tag) {\n        return (e) => {\n            let newTags = this.state.tags\n            let idx = newTags.indexOf(tag)\n            delete newTags[idx]\n            // debugger;\n            this.setState({ tags: newTags })\n        }\n    }\n\n    render() {\n        let tags;\n\n        if (this.state.tags){\n            tags = \n                this.state.tags.map((tag, idx) => {\n                    if (tag != null) {\n                        return (\n                            <div key={idx} className=\"new-dream-tags-item-container\" onClick={this.removeTag(tag)}>\n                                <div className=\"new-dream-tags-item-circle\" ></div>\n                                <p className=\"new-dream-tags-item\" >{tag}</p>\n                            </div>\n                        )\n                    } else {\n                        return null;\n                    }\n                })\n            \n        }\n\n\n        return (\n            <div className=\"new-dream-container\" onClick={e => e.stopPropagation()} >\n                <h1 className=\"new-dream-header\" >{Object.values(this.props.info).length === 0 ? \"New Dream\" : \"Edit Dream\"}</h1>\n                <form className=\"new-dream-radio\">\n                    <label className=\"new-dream-radio-btn\" >\n                        <h1 className={this.state.selectedOption === 'dream' ? \"new-dream-radio-btn-header-checked\" : \"new-dream-radio-btn-header\"} >Dream</h1>\n                        <input\n                            type=\"radio\"\n                            value=\"dream\"\n                            checked={this.state.selectedOption === \"dream\"}\n                            onChange={this.handleChange('selectedOption')}\n                        />\n                    </label>\n                    <label className=\"new-dream-radio-btn\" >\n                        <h1 className={this.state.selectedOption === 'goal' ? \"new-dream-radio-btn-header-checked\" : \"new-dream-radio-btn-header\"} >Goal</h1>\n                        <input\n                            type=\"radio\"\n                            value=\"goal\"\n          